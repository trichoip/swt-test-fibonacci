<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="test-login-update-search" verbose="2" annotations="JDK">
    <listeners>
        <listener class-name="com.swt301.testng.selenium.LoginListener"></listener>
    </listeners>
    <test name="login-success" preserve-order="true">
        <classes>
            <class name="com.swt301.testng.selenium.LoginTest_Success" />
        </classes>
    </test>
    <test name="login-failed">
        <classes>
            <class name="com.swt301.testng.selenium.LoginTest_Failed" />
        </classes>
    </test>
</suite>